/* Main font */

$Font
  font-family: $base-font-family



/* Icon font */

@font-face
  font-connect($base-icon-font, "/fonts/" + $base-icon-font)

$Iconfont
  font-family: $base-icon-font
  speak: none
  font-style: normal
  font-weight: normal
  font-variant: normal
  text-transform: none
  line-height: 1

[class^="icon-"]
[class*=" icon-"]
  @extend $Iconfont




/* Rub sign */

@font-face
  font-family: "ALSRubl-Tahoma"
  src: url("data:application/x-font-woff;charset=utf-8;base64,") format("woff")
  font-weight: normal
  font-style: normal

@font-face
  font-family: "ALSRubl-Tahoma-Bold"
  src: url("data:application/x-font-woff;charset=utf-8;base64,") format("woff")
  font-weight: normal
  font-style: normal

.rub
  font-family: "ALSRubl-Tahoma", $base-font-family

.rub-bold
  font-family: "ALSRubl-Tahoma-Bold", $base-font-family

.rub
.rub-bold
  &:after
    content: "\0420"